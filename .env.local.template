# Local Development Environment
# Copy this to .env for local development

# CORS Settings for local development
STORE_CORS=http://localhost:3000,http://localhost:8000
ADMIN_CORS=http://localhost:7000,http://localhost:7001
AUTH_CORS=http://localhost:7000,http://localhost:7001

# Your Cloud Database (replace with your actual connection strings)
DATABASE_URL=postgresql://your-cloud-db-connection-string
REDIS_URL=redis://your-cloud-redis-connection-string

# Development secrets (use different ones in production)
JWT_SECRET=dev-super-secret-jwt-key
COOKIE_SECRET=dev-super-secret-cookie-key

# Database name
DB_NAME=cubitstore-dev

# Port for local development
PORT=9000 