{
  "name": "cubitstore-backend",
  "version": "1.0.0",
  "description": "CubitStore B2B Backend API",
  "author": "CubitStore",
  "license": "MIT",
  "keywords": [
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa",
    "b2b"
  ],
  "scripts": {
    "build": "medusa build",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start",
    "start:prod": "NODE_ENV=production medusa start",
    "dev": "medusa develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin": "1.13.0",
    "@medusajs/cache-redis": "1.13.0",
    "@medusajs/event-bus-local": "1.13.0",
    "@medusajs/event-bus-redis": "1.13.0",
    "@medusajs/framework": "1.13.0",
    "@medusajs/medusa": "1.19.0",
    "@medusajs/modules-sdk": "1.13.0",
    "@medusajs/stock-location": "1.13.0",
    "@medusajs/types": "1.13.0",
    "@medusajs/utils": "1.13.0",
    "@medusajs/workflows-sdk": "^1.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "openai": "^4.28.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.8.4",
    "@medusajs/ui-preset": "2.8.4",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vite": "^5.2.11"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@4.4.0"
}
